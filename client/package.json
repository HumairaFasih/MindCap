  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb/hooks"
    ],
    "rules": {
      "react/prop-types": "off",
      "camelcase": "off",
      "no-nested-ternary": "off",
      "prettier/prettier": [
        "warn",
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "printWidth": 80,
          "endOfLine": "lf"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-wesbos": "^3.2.3"
  }
