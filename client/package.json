{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/joy": "^5.0.0-alpha.72",
    "@mui/lab": "^5.0.0-alpha.124",
    "@mui/material": "^5.11.13",
    "@mui/system": "^5.11.14",
    "@mui/x-date-pickers": "^6.0.3",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
